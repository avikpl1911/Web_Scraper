{
    "manifest_version":2,
    "version":"1.0",
    "name":"web_scraper",
    "content_scripts":[
     {
      "matches":["<all_urls>"],
      "js":["main.js"]
     }
    ],
    "background":{
      "scripts":["background.js"]
    },
    "browser_action":{
      "default_popup":"popup/index.html",
      "default_title":"obscenity blocker"
      
    },
    
    "web_accessible_resources": [
          "pp.jpg"
    ],
    "permissions": [
      "storage"
  ]
  }
  
 
      